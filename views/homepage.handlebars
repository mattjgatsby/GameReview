<!--Carousel for home page-->
<div
  id="carouselExampleIndicators"
  class="carousel slide container-sm"
  data-ride="carousel"
>

  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="{{displayData.0.image}}" class="d-block w-100" alt="..." />
    </div>
    <div class="carousel-item">
      <img src="{{displayData.1.image}}" class="d-block w-100" alt="..." />
    </div>
    <div class="carousel-item">
      <img src="{{displayData.2.image}}" class="d-block w-100" alt="..." />
    </div>
    <div class="carousel-item">
      <img src="{{displayData.3.image}}" class="d-block w-100" alt="..." />
    </div>
    <div class="carousel-item">
      <img src="{{displayData.4.image}}" class="d-block w-100" alt="..." />
    </div>
  </div>
  <button
    class="carousel-control-prev"
    type="button"
    data-target="#carouselExampleIndicators"
    data-slide="prev"
  >
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </button>
  <button
    class="carousel-control-next"
    type="button"
    data-target="#carouselExampleIndicators"
    data-slide="next"
  >
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </button>
</div>